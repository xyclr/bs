<!DOCTYPE html>
<!-- saved from url=(0044)http://webapplayers.com/inspinia_admin-v2.1/ -->
<html>
<head>
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">

  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">

  <title><%= title %> | Dashboard</title>

  <link href="http://www.ndesig.com/css/bootstrap.min.css" rel="stylesheet">
  <link href="http://www.ndesig.com/font-awesome/css/font-awesome.css" rel="stylesheet">

  <!-- Toastr style -->
  <link href="http://www.ndesig.com/css/plugins/toastr/toastr.min.css" rel="stylesheet">

  <!-- Gritter -->
  <link href="http://www.ndesig.com/js/plugins/gritter/jquery.gritter.css" rel="stylesheet">

  <link href="http://www.ndesig.com/css/animate.css" rel="stylesheet">
  <link href="http://www.ndesig.com/css/style.css" rel="stylesheet">

  <!-- Mainly scripts -->
  <script async="" src="http://www.ndesig.com/js/analytics.js"></script>
  <script src="http://www.ndesig.com/js/jquery-2.1.1.js"></script>
  <script src="http://www.ndesig.com/js/bootstrap.min.js"></script>
  <script src="http://www.ndesig.com/js/jquery.metisMenu.js"></script>
  <script src="http://www.ndesig.com/js/jquery.slimscroll.min.js"></script>
  <script src="http://www.ndesig.com/js/plugins/toastr/toastr.min.js" ></script>

  <script type="text/javascript">

    $(function(){
      //提示信息
      <% if (success) { %>
      toastr.success('<%= success %>');
      <% } %>
      <% if (error) { %>
      toastr.error('<%= error %>');
      <% } %>

      //导航设置
      var pageCat = window.location.pathname.split('/')[1];
      var siderLi = $("#side-menu>li:not('.nav-header')");
      if( pageCat.match(/archive|post|edit|category/ig) != null) {
        siderLi.removeClass('active').eq(1).addClass('active');
      } else if(pageCat.match(/file/ig) != null) {
        siderLi.removeClass('active').eq(2).addClass('active');
      } else if(pageCat.match(/sys/ig) != null) {
        siderLi.removeClass('active').eq(3).addClass('active');
      }


    })

  </script>
</head>
	  